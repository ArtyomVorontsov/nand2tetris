
class Dino {

	field Bednij bednij;
	field boolean proceed;

	constructor Dino new() {
		let bednij = Bednij.new();

		return this;
	}

	method void run(){
		var Wall wall;
		let wall = Wall.new();
		let proceed = true;	

		do wall.spawnWall();
		
		while(proceed){
			do bednij.draw(6720);
			do wall.drawWalls();
			do Floor.draw();
			do Sys.wait(5);
			do Screen.clearScreen();
		}
		
		return;
	}



}

